{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1",
      "headers": { "Cache-Control": "max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
